	<messages>
		<message id="150" name="ICAROUS_FLIGHTPLAN_UPDATE">
		  <description>Flight plan information </description>
		  <field type="uint8_t" name="msgType">Flag, 0-new flight plan,1-append waypoints to existingflight plan</field> 
		  <field type="float" name="standoffDist">Standoff distance</field>
		  <field type="float" name="maxHorDev">Maximum horizontal deviation from flight plan</field>
		  <field type="float" name="maxVerDev">Maximum vertical deviation from flight plan</field>
		  <field type="uint32_t" name="numWaypoints">Number of waypoints</field>
		  <field type="float[4]" name="wp1">Waypoint 1</field>
		  <field type="float[4]" name="wp2">Waypoint 2</field>
		  <field type="float[4]" name="wp3">Waypoint 3</field>
		  <field type="float[4]" name="wp4">Waypoint 4</field>
		  <field type="float[4]" name="wp5">Waypoint 5</field>
		  <field type="float[4]" name="wp6">Waypoint 6</field>
		  <field type="float[4]" name="wp7">Waypoint 7</field>
		  <field type="float[4]" name="wp8">Waypoint 8</field>
		  <field type="float[4]" name="wp9">Waypoint 9</field>
		  <field type="float[4]" name="wp10">Waypoint 10</field>
		</message>

		<message id="151" name="ICAROUS_GEOFENCE">
		  <description>Geo fence information</description>
		  <field type="uint8_t" name="msgType">Flag, 0-new fence, 1-append coordinates to existing fence, 2-remove existing fence</field>
		  <field type="uint8_t" name="fenceType">Flag, 0-keep in, 1-keep out</field>
		  <field type="uint8_t" name="fenceID"> Geo-fence ID</field>
		  <field type="float" name="fenceFloor"> Fence floor height </field>
		  <field type="float" name="fenceCeiling"> Fence ceiling height </field>
		  <field type="float" name="numVertices"> Number of polygon verticies </field>
		  <field type="float[3]" name="vertex1">Vertex1</field>
		  <field type="float[3]" name="vertex2">Vertex2</field>
		  <field type="float[3]" name="vertex3">Vertex3</field>
		  <field type="float[3]" name="vertex4">Vertex4</field>
		  <field type="float[3]" name="vertex5">Vertex5</field>
		  <field type="float[3]" name="vertex6">Vertex6</field>
		</message>

		<message id="152" name="ICAROUS_MISSION_START_STOP">
		  <description>Flag to start/stop mission</description>
		  <field type="uint8_t" name="missionStart"> mission start (true)/stop (false)</field>
		</message>

		<message id="153" name="COMBOX_PULSE">
		  <description>Combox heartbeat</description>
		  <field type="uint64_t" name="count">count</field>
