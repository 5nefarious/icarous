// -*- Mode: Java -*-
// Simple mission to turn to different headings when flying
Boolean Command SetYaw(Real heading);
MISSION:
{
	Real Heading = 0;
	In Boolean execResolution;
	
	MISSION_OPS:{
	    Start !execResolution;
	    Repeat true;
	    
	    Heading = Heading + 10;
	    
	    
	    SetYaw(Heading);
	    
	    
	    Wait 2.0;
	}
}
